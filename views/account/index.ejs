<% if (title) { %>
<h1 id="heading"><%= title %></h1>
<% } else {
  res.redirect('/')
} %>
<%- messages()  %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>
 <% if (typeof accountData !== 'undefined' && accountData &&  accountData.account_type !== 'Client') { %>
<h2>Welcome <%= accountData.account_firstname %></h2>
<% } %>
<p>You're logged in</p>
<a href="edit">Edit Account Information</a>